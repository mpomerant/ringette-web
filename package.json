{
  "name": "web-app",
  "version": "1.0.0",
  "description": "A test harness to serve and validate the web component",
  "author": {
    "name": "Oracle AMPLIFY Team"
  },
  "dependencies": {
    "express": "^4.14.0",
    "get-image-colors": "^1.8.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-connect": "^5.6.1",
    "gulp-connect-proxy": "^0.3.1",
    "gulp-eslint": "^4.0.1",
    "gulp-eslint-if-fixed": "^1.0.0",
    "gulp-git": "^2.5.0",
    "gulp-header": "^1.8.10",
    "gulp-requirejs-optimize": "^1.3.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-watch": "^5.0.1",
    "http-proxy-middleware": "^0.18.0",
    "proxy-middleware": "^0.15.0",
    "sanitize-filename": "^1.6.1",
    "yargs": "^6.0.0"
  },
  "scripts": {
    "start": "nohup nodemon bin/www > stdout.txt 2> stderr.txt &",
    "build": "gulp copy"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "amd": true,
      "jasmine": true,
      "browser": true
    },
    "globals": {
      "Promise": true
    },
    "rules": {
      "block-scoped-var": "error",
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-console": "warn"
    }
  },
  "private": true,
  "devDependencies": {
    "gulp-replace": "^0.5.4"
  }
}
